{
    "include": ["server", "assets.d.ts"],
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "CommonJS",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true
    }
}
